<config>
    <application title="TACCIMO GIS Viewer">
    </application>
    <extent xmin="-15000000" ymin="2000000" xmax="-6000000" ymax="7000000" xminZoom="-15000000" yminZoom="2000000" xmaxZoom="-6000000" ymaxZoom="7000000" wkid="102100">
    </extent>
    <server map_path="http://arcgisserver7.nemac.org/ArcGIS/">
    </server>
    <images>
        <image label="Streets" name="Streets" selected="true" url="http://server.arcgisonline.com/ArcGIS/rest/services/World_Street_Map/MapServer" />
        <image label="Basic" name="Basic" url="http://services.arcgisonline.com/ArcGIS/rest/services/Canvas/World_Light_Gray_Base/MapServer" />
        <image label="Relief" name="Relief" url="http://server.arcgisonline.com/ArcGIS/rest/services/World_Shaded_Relief/MapServer" />
        <image label="Terrain" name="Terrain" url="http://server.arcgisonline.com/ArcGIS/rest/services/World_Terrain_Base/MapServer" />
        <image label="Imagery" name="Imagery" url="http://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer" />
        <image label="Topo Map" name="Topo_Map" url="http://services.arcgisonline.com/ArcGIS/rest/services/World_Topo_Map/MapServer" />
        <image label="USA Topo" name="USA_Topo" url="http://server.arcgisonline.com/ArcGIS/rest/services/USA_Topo_Maps/MapServer" />
        <image label="None" name="None" url="http://arcgisserver7.nemac.org/ArcGIS/rest/services/US/US_Jurisdictions_filled2/MapServer/" />
    </images>
    <mapviews>
        <view label="General" name="General">
            <viewGroup name="Base Layers" />
            <viewGroup name="PRISM HISTORIC" />
        </view>
        <view label="Climate Change Atlas" name="Climate Change Atlas" selected="true">
            <viewGroup name="Base Layers" />
            <viewGroup name="Climate Change Atlas"/>
        </view>
    </mapviews>
    <wmsLayers>
        <wmsGroup gid="G01" label="Base Layers" name="Base Layers">
            <wmsSubgroup label="TACCIMO Base Layers">
                <wmsLayer identify="true" layers="9" legend="http://www.taccimogis.sgcp.ncsu.edu/TACCIMODevGIS/TACCIMO_Beta/GIS/assets/Mashup_Legends/TACCIMO/TACCIMO_BaseLayers_2013/National_Forests.PNG" lid="NF" name="National Forests" srs="EPSG:102100" styles="default" url="http://wassimap.sgcp.ncsu.edu:8399/arcgis/services/taccimo/TACCIMO_baselayers/MapServer/WMSServer" visible="true" /> 
                <wmsLayer identify="true" layers="8" legend="http://www.taccimogis.sgcp.ncsu.edu/TACCIMODevGIS/TACCIMO_Beta/GIS/assets/Mashup_Legends/TACCIMO/TACCIMO_BaseLayers_2013/Experimental_Forests.PNG" lid="EF" name="Experimental Forests" srs="EPSG:102100" styles="default" url="http://wassimap.sgcp.ncsu.edu:8399/arcgis/services/taccimo/TACCIMO_baselayers/MapServer/WMSServer" visible="true" /> 
                <wmsLayer identify="true" layers="7" legend="http://www.taccimogis.sgcp.ncsu.edu/TACCIMODevGIS/TACCIMO_Beta/GIS/assets/Mashup_Legends/TACCIMO/TACCIMO_BaseLayers_2013/States.PNG" lid="STA" name="States" srs="EPSG:102100" styles="default" url="http://wassimap.sgcp.ncsu.edu:8399/arcgis/services/taccimo/TACCIMO_baselayers/MapServer/WMSServer" visible="true" /> 
                <wmsLayer identify="true" layers="6" legend="http://www.taccimogis.sgcp.ncsu.edu/TACCIMODevGIS/TACCIMO_Beta/GIS/assets/Mashup_Legends/TACCIMO/TACCIMO_BaseLayers_2013/Counties.PNG" lid="CNTY" name="Counties" srs="EPSG:102100" styles="default" url="http://wassimap.sgcp.ncsu.edu:8399/arcgis/services/taccimo/TACCIMO_baselayers/MapServer/WMSServer" visible="true" /> 
                <wmsLayer identify="true" layers="5" legend="http://www.taccimogis.sgcp.ncsu.edu/TACCIMODevGIS/TACCIMO_Beta/GIS/assets/Mashup_Legends/TACCIMO/TACCIMO_BaseLayers_2013/Watersheds_(12-digit_HUC).PNG" lid="WS12" name="Watersheds (12 Digit HUC)" srs="EPSG:102100" styles="default" url="http://wassimap.sgcp.ncsu.edu:8399/arcgis/services/taccimo/TACCIMO_baselayers/MapServer/WMSServer" visible="true" /> 
                <wmsLayer identify="true" layers="4" legend="http://www.taccimogis.sgcp.ncsu.edu/TACCIMODevGIS/TACCIMO_Beta/GIS/assets/Mashup_Legends/TACCIMO/TACCIMO_BaseLayers_2013/USFS_Regions.PNG" lid="USFS" name="USFS Regions" srs="EPSG:102100" styles="default" url="http://wassimap.sgcp.ncsu.edu:8399/arcgis/services/taccimo/TACCIMO_baselayers/MapServer/WMSServer" visible="true" /> 
                <wmsLayer identify="true" layers="3" legend="http://www.taccimogis.sgcp.ncsu.edu/TACCIMODevGIS/TACCIMO_Beta/GIS/assets/Mashup_Legends/TACCIMO/TACCIMO_BaseLayers_2013/TACCIMO_Subregions.PNG" lid="TSR" name="TACCIMO Subegions" srs="EPSG:102100" styles="default" url="http://wassimap.sgcp.ncsu.edu:8399/arcgis/services/taccimo/TACCIMO_baselayers/MapServer/WMSServer" visible="true" /> 
                <wmsLayer identify="true" layers="2" legend="http://www.taccimogis.sgcp.ncsu.edu/TACCIMODevGIS/TACCIMO_Beta/GIS/assets/Mashup_Legends/TACCIMO/TACCIMO_BaseLayers_2013/Ecomap2007_-_Provinces.PNG" lid="EM" name="Ecomap2007 - Provinces" srs="EPSG:102100" styles="default" url="http://wassimap.sgcp.ncsu.edu:8399/arcgis/services/taccimo/TACCIMO_baselayers/MapServer/WMSServer" visible="true" /> 
                <wmsLayer identify="true" layers="1" legend="http://www.taccimogis.sgcp.ncsu.edu/TACCIMODevGIS/TACCIMO_Beta/GIS/assets/Mashup_Legends/TACCIMO/TACCIMO_BaseLayers_2013/Protected_Areas_(PAD-US).PNG" lid="PA" name="Protected Areas (PAD-US)" srs="EPSG:102100" styles="default" url="http://wassimap.sgcp.ncsu.edu:8399/arcgis/services/taccimo/TACCIMO_baselayers/MapServer/WMSServer" visible="true" /> 
            </wmsSubgroup>
        </wmsGroup>
        <wmsGroup gid="G02" label="PRISM Historic" name="PRISM HISTORIC">
            <wmsSubgroup label="PRISM Historic Layers">
                <wmsLayer identify="true" layers="2" legend="http://www.taccimogis.sgcp.ncsu.edu/TACCIMODevGIS/TACCIMO_Beta/GIS/assets/Mashup_Legends/TACCIMO/PRISM_Historic/Historic_Precipitation_(mm,_1970-2000).PNG" lid="HP" name="Historic Precipitation" srs="EPSG:102100" styles="default" url="http://www.taccimogis.sgcp.ncsu.edu/ArcGIS/services/TACCIMO/Prism_Historic/MapServer/WMSServer" visible="true" />
                <wmsLayer identify="true" layers="1" legend="http://www.taccimogis.sgcp.ncsu.edu/TACCIMODevGIS/TACCIMO_Beta/GIS/assets/Mashup_Legends/TACCIMO/PRISM_Historic/Historic_Temperature_(C,_1970-2000).PNG" lid="HT" name="Historic Temperature" srs="EPSG:102100" styles="default" url="http://www.taccimogis.sgcp.ncsu.edu/ArcGIS/services/TACCIMO/Prism_Historic/MapServer/WMSServer" visible="true" /> 
            </wmsSubgroup>
        </wmsGroup>
        <wmsGroup gid="G03" label="Climate Change Atlas" name="Climate Change Atlas">
            <wmsSubgroup label="Climate Model" type="radiobutton">
                <wmsLayer selected="true" identify="false" layers="1" legend="" lid="CTA" name="Current (Historic)" srs="" styles="default" url="" visible="true" />
                <wmsLayer identify="false" layers="50" legend="" lid="A1F1TA" name="GCM Average High (A1F1)" srs="" styles="default" url="" visible="true" /> 
                <wmsLayer identify="false" layers="99" legend="" lid="B1TA" name="GCM Average Low (B1)" srs="" styles="default" url="" visible="true" /> 
            </wmsSubgroup>
            <wmsSubgroup label="Tree Species" type="dropdownbox" selected="true">
                <wmsLayer identify="true" layers="46" legend="" lid="ABTA" name="American Basswood Tilia americana" srs="EPSG:102100" styles="default" url="http://wassimap.sgcp.ncsu.edu:8399/arcgis/services/taccimo/treeatlas_medium/MapServer/WMSServer" visible="true" /> 
                <wmsLayer identify="true" layers="27" legend="" lid="ABFG" name="American Beech Fagus grandifolia" srs="EPSG:102100" styles="default" url="http://wassimap.sgcp.ncsu.edu:8399/arcgis/services/taccimo/treeatlas_medium/MapServer/WMSServer" visible="true" /> 
            </wmsSubgroup>
        </wmsGroup>
    </wmsLayers>
    <tools>
        <tool label="Print Map" name="Print" />
        <tool label="Export Map to JPG" name="Export" />
        <tool label="Identify Features" linkFields="" name="Identify" />
        <tool helpText="Click the map to see NDVI for the point. Or, enter coordinates and click Make Graph." label="Graph NDVI" name="Phenograph" />
        <tool helpText="Copy the URL below to share this map with someone else." label="Share this Map" name="ShareMap" />
    </tools>
</config>
